{
  "outputDirectory": ".",
  "functions": {
    "api/github-auth.js": {
      "maxDuration": 10
    },
    "api/test.js": {
      "maxDuration": 10
    }
  },
  "redirects": [
    { "source": "/team", "destination": "/about.html", "permanent": true },
    { "source": "/team/", "destination": "/about.html", "permanent": true },
    { "source": "/team/:slug", "destination": "/people/:slug", "permanent": true },
    { "source": "/team/:slug/", "destination": "/people/:slug", "permanent": true },
    { "source": "/profile", "destination": "/about.html", "permanent": true },
    { "source": "/profile/", "destination": "/about.html", "permanent": true },
    { "source": "/profile/:slug", "destination": "/people/:slug", "permanent": true },
    { "source": "/profile/:slug/", "destination": "/people/:slug", "permanent": true },
    { "source": "/people", "destination": "/about.html", "permanent": true },
    { "source": "/people/", "destination": "/about.html", "permanent": true }
  ],
  "rewrites": [
    { "source": "/blog", "destination": "/blog/index.html" },
    { "source": "/blog/", "destination": "/blog/index.html" },
    { "source": "/blog/:slug", "destination": "/blog-post.html?slug=:slug" },
    { "source": "/blog/:slug/", "destination": "/blog-post.html?slug=:slug" },
    { "source": "/blog-post", "destination": "/blog-post/index.html" },
    { "source": "/blog-post/", "destination": "/blog-post/index.html" },
    { "source": "/subscribe", "destination": "/subscribe/index.html" },
    { "source": "/subscribe/", "destination": "/subscribe/index.html" },
    { "source": "/bloomly-ai", "destination": "/bloomly-ai/index.html" },
    { "source": "/bloomly-ai/", "destination": "/bloomly-ai/index.html" },
    { "source": "/people/:slug", "destination": "/people/index.html?slug=:slug" },
    { "source": "/people/:slug/", "destination": "/people/index.html?slug=:slug" }
  ]
}

